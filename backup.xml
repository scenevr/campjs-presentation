  <spawn position="0 0 2" />
  <skybox style="color: linear-gradient(#fff, #acf)" />

  <box id="next-button" style="color: #0f0" position="2 0.9 0.4" scale="0.4 0.4 0.5" />
  <box style="color: #fff" position="2 0.6 0.4" scale="0.6 1.2 0.2" />

  <box id="prev-button" style="color: #f00" position="-2 0.9 0.4" scale="0.4 0.4 0.5" />
  <box style="color: #fff" position="-2 0.6 0.4" scale="0.6 1.2 0.2" />

  <script>
      function reposition (start) {
        var elements = document.getElementsByTagName('billboard')

        var p = new Vector(0 - start * 4, 1.5, 0)
        var r = new Euler(0, 0, 0)

        elements.forEach(function (el) {
          el.position.copy(p)
          el.rotation.copy(r)

          // console.log(el)

          var m = new Vector(4, 0, 0).applyEuler(r)

          p.add(m)

          if (p.x > 0) {
            r.y -= 0.1
          }
        })
      }

      var index = 0

      document.getElementById('prev-button').addEventListener('click', function (e) {
        index -= 1
        reposition(index)
      })

      document.getElementById('next-button').addEventListener('click', function (e) {
        index += 1
        reposition(index)
      })

      reposition(index)

      var y = 0

      setInterval(function () {
      
      })
  </script>

